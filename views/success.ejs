<%- include('./partials/head') %>
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Resultado</title>
</head>
<body>
  <div>
    <div class="container py-5 h-100">
      <div class="row d-flex justify-content-center align-items-center h-100">
        <div class="col-xl-10">
          <div class="card rounded-3 text-black">
            <div class="container">
              <div class="card-body p-md-5 mx-md-4">
                <h2 class="text-center">Resultados de Formulario signo del Zodiaco</h2>

                <main>
                  <div class="container my-3">
                      <h3 class=" my-2">Descargar resultados</h3>
                      <h3>Total de usuarios: <%= users.length %></h3>
                      <div class=" row p-4">
                              
                              <div class=" actions card text-center col-md my-2 p-5">
                                  <div class=" action-item card-body p-3">
                                    <h5 class="card-title"><a class="btn btn-info btn-rounded" href="/download/excel" role="button">Descargar Exel <i class="fa-solid fa-file-excel"></i></a> </h5>
                                    
                                  </div> 
                                </div>
                          
                          
                              <div class=" actions card text-center col-md my-2 p-5">
                                  <div class=" action-item card-body">
                                    <h5 class="card-title"><a class="btn btn-info btn-rounded" href="/download/csv" role="button">Guardar CSV <i class="fa-solid fa-file-csv"></i></a></h5>
                                    
                                  </div> 
                                </div>
                      </div>
                  </div>
              </main>

              </div>
            </div>
          </div>

        </div>
      </div>
    </div>
  </div>

</body>
</html>
